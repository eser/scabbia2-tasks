{
    "name": "scabbiafw/scabbia2-tasks",
    "description": "Scabbia2 Tasks Component",
    "type": "library",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "preferred-install": "dist"
    },
    "keywords": [
        "larukedi",
        "scabbia",
        "scabbia2",
        "scabbiafw",
        "tasks",
        "commands"
    ],
    "homepage": "http://www.scabbiafw.com/",
    "license": [
        "Apache-2.0"
    ],
    "authors": [
        {
            "name": "Scabbia Framework Organization",
            "homepage": "http://scabbiafw.com/"
        },
        {
            "name": "Contributors",
            "homepage": "https://github.com/scabbiafw/scabbia2-tasks/blob/master/contributors.md"
        }
    ],
    "support": {
        "issues": "https://github.com/scabbiafw/scabbia2-tasks/issues",
        "source": "https://github.com/scabbiafw/scabbia2-tasks"
    },
    "require": {
        "php": ">=5.6.0",
        "scabbiafw/scabbia2-formatters": "*"
    },
    "require-dev": {
        "scabbiafw/scabbia2-testing": "*"
    },
    "suggest": {
    },
    "autoload": {
        "psr-4": {
            "Scabbia\\Tasks\\": [ "src/Tasks/" ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Scabbia\\Tests\\Tasks\\": [ "tests/Tasks/" ]
        }
    },
    "bin": [
        "bin/scabbia"
    ]
}
